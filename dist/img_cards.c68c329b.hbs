{{#each this}}
<div class="photo-card">
    <img src="{{webformatURL}}" alt="" />


    <div class="stats">
        <p class="stats-item">
            <i class="material-icons">thumb_up{{likes}}</i>

        </p>
        <p class="stats-item">
            <i class="material-icons">visibility{{views}}</i>

        </p>
        <p class="stats-item">
            <i class="material-icons">comment{{comments}}</i>

        </p>
        <p class="stats-item">
            <i class="material-icons">cloud_download{{downloads}}</i>

        </p>
    </div>
</div>
{{/each}}